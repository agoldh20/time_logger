<div>
  <% @time_logs.each do |time_log| %>
    <%= time_log.sprint %>
    <%= time_log.ticket %>
    <%= time_log.action %>
    <%= time_log.start_time %>
    <%= time_log.end_time %>
    <%= time_log.notes %>
    <% if time_log.currently_active_task %>
      <button>End Button</button>
    <% end %>
  <% end %>
</div>